{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["manifest.yml"],
      "matchStrings": ["docker: (?<currentValue>.+)"],
      "datasourceTemplate": "custom.docker",
      "depNameTemplate": "docker"

    }
  ],
    "packageRules": [
      {
        "matchPackageNames": ["docker"],
        "extractVersion": "docker-ce_(?<version>.+)-1~ubuntu\\.22\\.04~jammy_amd64\\.deb"
      }
    ],
  "customDatasources": {
    "custom.docker": {
      "defaultRegistryUrlTemplate": "https://download.docker.com/linux/ubuntu/dists/jammy/pool/stable/amd64",
      "format": "html"
    }
  }
}
