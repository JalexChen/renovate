{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "repositories": ["JalexChen/renovate"],
  "groupName": "ubuntu-2204",
  "groupSlug": "ubuntu-2204-all",
  "customDatasources": {
    "docker-jammy": {
      "defaultRegistryUrlTemplate": "https://download.docker.com/linux/ubuntu/dists/jammy/pool/stable/amd64/",
      "format": "html"
    }
  },
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["manifest2.yml"],
      "matchStrings": ["docker: 5:(?<currentValue>.+)"],
      "datasourceTemplate": "custom.docker-jammy",
      "depNameTemplate": "docker",
      "versioningTemplate": "deb"
    },
    {
      "customType": "regex",
      "fileMatch": ["manifest2.yml"],
      "matchStrings": ["containerd: (?<currentValue>.+)"],
      "datasourceTemplate": "custom.docker-jammy",
      "depNameTemplate": "containerd",
      "versioningTemplate": "deb"
    },
    {
      "customType": "regex",
      "fileMatch": ["manifest2.yml"],
      "matchStrings": ["docker-compose: (?<currentValue>.+)"],
      "datasourceTemplate": "custom.docker-jammy",
      "depNameTemplate": "docker-compose",
      "versioningTemplate": "deb"
    },
    {
      "customType": "regex",
      "fileMatch": ["manifest2.yml"],
      "matchStrings": ["go: (?<currentValue>.+)"],
      "depNameTemplate": "go",
      "datasourceTemplate": "golang-version"
    },
    {
      "customType": "regex",
      "fileMatch": ["manifest2.yml"],
      "matchStrings": ["nodelts: (?<currentValue>20.+)"],
      "depNameTemplate": "nodelts",
      "datasourceTemplate": "node",
      "versioningTemplate": "node"
    },
    {
      "customType": "regex",
      "fileMatch": ["manifest2.yml"],
      "matchStrings": ["nodeCurrent: (?<currentValue>21.+)"],
      "depNameTemplate": "nodeCurrent",
      "datasourceTemplate": "node",
      "versioningTemplate": "node"
    },
    {
      "customType": "regex",
      "fileMatch": ["manifest2.yml"],
      "matchStrings": ["gcloud: (?<currentValue>.+)"],
      "depNameTemplate": "twistedpair/google-cloud-sdk",
      "datasourceTemplate": "github-tags"
    },
    {
      "customType": "regex",
      "fileMatch": ["manifest2.yml"],
      "matchStrings": ["git_lfs: (?<currentValue>.+)"],
      "depNameTemplate": "git-lfs/git-lfs",
      "datasourceTemplate": "github-releases",
      "extractVersionTemplate": "^v(?<version>.*)$"
    },
    {
      "customType": "regex",
      "fileMatch": ["manifest2.yml"],
      "matchStrings": ["gradle: (?<currentValue>.+)"],
      "depNameTemplate": "gradle",
      "datasourceTemplate": "gradle-version"
    },
    {
      "customType": "regex",
      "fileMatch": ["manifest2.yml"],
      "matchStrings": ["lein: (?<currentValue>.+)"],
      "depNameTemplate": "technomancy/leiningen",
      "datasourceTemplate": "github-releases"
    },
    {
      "customType": "regex",
      "fileMatch": ["manifest2.yml"],
      "matchStrings": ["pyenv: (?<currentValue>.+)"],
      "depNameTemplate": "pyenv/pyenv",
      "datasourceTemplate": "github-releases",
      "extractVersionTemplate": "^v(?<version>.*)$"
    },
    {
      "customType": "regex",
      "fileMatch": ["manifest2.yml"],
      "matchStrings": ["python3: (?<currentValue>.+)"],
      "depNameTemplate": "python/cpython",
      "datasourceTemplate": "github-tags",
      "extractVersionTemplate": "^v(?<version>.*)$"
    },
    {
      "customType": "regex",
      "fileMatch": ["manifest2.yml"],
      "matchStrings": ["ruby: (?<currentValue>.+)"],
      "depNameTemplate": "ruby",
      "datasourceTemplate": "ruby-version"
    },
    {
      "customType": "regex",
      "fileMatch": ["manifest2.yml"],
      "matchStrings": ["sbt: (?<currentValue>.+)"],
      "depNameTemplate": "sbt/sbt",
      "datasourceTemplate": "github-releases",
      "extractVersionTemplate": "^v(?<version>.*)$"
    },
    {
      "customType": "regex",
      "fileMatch": ["manifest2.yml"],
      "matchStrings": ["yq: (?<currentValue>.+)"],
      "depNameTemplate": "mikefarah/yq",
      "datasourceTemplate": "github-releases",
      "extractVersionTemplate": "^v(?<version>.*)$"
    },
    {
      "customType": "regex",
      "fileMatch": ["manifest2.yml"],
      "matchStrings": ["maven: (?<currentValue>.*)"],
      "depNameTemplate": "apache/maven",
      "datasourceTemplate": "github-releases",
      "extractVersionTemplate": "^maven-(?<version>.*)$"
    }
  ],
  "packageRules": [
    {
      "matchDatasources": ["custom.docker-jammy"],
      "matchDepNames": ["docker"],
      "extractVersion": "docker-ce_(?<version>.+)_amd64\\.deb$"
    },
    {
      "matchDatasources": ["custom.docker-jammy"],
      "matchDepNames": ["containerd"],
      "extractVersion": "containerd\\.io_(?<version>.+)_amd64\\.deb"
    },
    {
      "matchDatasources": ["custom.docker-jammy"],
      "matchDepNames": ["docker-compose"],
      "extractVersion": "docker-compose-plugin_(?<version>.+)_amd64\\.deb"
    },
    {
      "groupName": "ubuntu-2204",
      "groupSlug": "ubuntu-2204-all",
      "matchFileNames": ["hello/manifest2.yml"],
      "matchDatasources": [
        "custom.docker-jammy",
        "github-releases",
        "github-tags",
        "node",
        "ruby-version",
        "golang-version",
        "gradle-version"
      ]
    }
  ],
  "separateMajorMinor": false
}
