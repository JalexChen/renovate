{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:recommended"],
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["manifest.yml"],
      "matchStrings": ["docker: 5:(?<currentValue>)"],
      "datasourceTemplate": "custom.docker",
      "depNameTemplate": "docker"

    }
  ],
    "packageRules": [
      {
        "matchPackageNames": ["docker"],
        "extractVersion": "\\d+\\.\\d+\\.\\d+-\\d+~ubuntu\\.\\d+\\.\\d+~\\w+"
      }
    ],
  "customDatasources": {
    "custom.docker": {
      "defaultRegistryUrlTemplate": "https://download.docker.com/linux/ubuntu/dists/jammy/pool/stable/amd64/",
      "format": "html"
    }
  }
}
